                  <activityDefId>email_on_received</activityDefId>
                  <pluginName>org.joget.apps.app.lib.BeanShellTool</pluginName>
                  <pluginProperties>{&quot;script&quot;:&quot;import java.util.Map;\nimport java.io.IOException;\nimport javax.servlet.http.HttpServletRequest;\nimport org.apache.http.HttpResponse;\nimport org.apache.http.client.methods.HttpGet;\nimport org.apache.http.impl.client.CloseableHttpClient;\nimport org.apache.http.impl.client.HttpClients;\nimport org.apache.http.util.EntityUtils;\nimport org.joget.commons.util.StringUtil;\nimport org.joget.commons.util.LogUtil;\nimport org.joget.workflow.util.WorkflowUtil;\nimport org.joget.workflow.model.WorkflowAssignment;\nimport org.joget.apps.app.model.AppDefinition;\nimport org.joget.plugin.property.service.PropertyUtil;\n\n\npublic Object execute(WorkflowAssignment wfAssignment, AppDefinition appDef, HttpServletRequest request) {\n    String jsonUrl = \&quot;http:\/\/localhost:8081\/jw\/web\/json\/workflow\/assignment\/list\/pending?processId=testGit2:latest:request_approver_process\&quot;;\n    CloseableHttpClient client = null;\n    HttpGet get = null;\n    try {\n        client = HttpClients.createDefault();\n    \n        jsonUrl = WorkflowUtil.processVariable(jsonUrl, \&quot;\&quot;, wfAssignment);\n    \n        jsonUrl = StringUtil.encodeUrlParam(jsonUrl);\n    \n        get = new HttpGet(jsonUrl);\n        HttpResponse response = client.execute(get);\n        String jsonResponse = EntityUtils.toString(response.getEntity(), \&quot;UTF-8\&quot;);\n    \n        LogUtil.info(\&quot;BeanShell.EmailOnReceived\&quot;, \&quot;jsonResponse : \&quot; + jsonResponse);\n        Map object = PropertyUtil.getPropertiesValueFromJson(jsonResponse);\n    \n        \/\/storeToForm(wfAssignment, properties, object);\n        \/\/storeToWorkflowVariable(wfAssignment, properties, object);\n    \n    } catch (Exception ex) {\n        LogUtil.error(\&quot;BeanShell.EmailOnReceived\&quot;, ex, \&quot;\&quot;);\n    } finally {\n        try {\n            if (get != null) {\n                get.releaseConnection();\n            }\n            if (client != null) {\n                client.close();\n            }\n        } catch (IOException ex) {\n            LogUtil.error(\&quot;BeanShell.EmailOnReceived\&quot;, ex, \&quot;\&quot;);\n        }\n    }\n    \n}\n\nreturn execute(workflowAssignment, appDef, request);&quot;}</pluginProperties>
               </packageActivityPlugin>
            </entry>
